<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.Group?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane fx:id="Level_Complete_Pop_Up" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ae2dms.UI.Game.PopUps.LevelCompletePopUpController">
    <center>
        <Group BorderPane.alignment="CENTER">
            <children>
                <Group>
                    <children>
                        <ImageView fitHeight="359.0" fitWidth="622.0">
                            <Image url="@../Assets/Game/LevelCompletePopUp/Level_Complete.png"/>
                        </ImageView>
                        <ImageView fx:id="Next_Level_Button" layoutX="230.0" layoutY="280.0" styleClass="Button">
                            <Image url="@../Assets/Game/LevelCompletePopUp/Next_Level_Button.png" />
                        </ImageView>
                        <BorderPane layoutX="81.0" layoutY="138.0" prefHeight="106.0" prefWidth="108.0">
                            <center>
                                <Label fx:id="Complete_Level_Index" text="Level" textFill="#ffcc00" BorderPane.alignment="CENTER">
                                    <font>
                                        <Font name="Smartisan Maquette Bold" size="80.0" />
                                    </font>
                                </Label>
                            </center>
                        </BorderPane>
                        <BorderPane layoutX="416.0" layoutY="138.0" prefHeight="106.0" prefWidth="146.0">
                            <center>
                                <Label fx:id="Complete_Time" text="Time" textFill="#ffb45c" BorderPane.alignment="CENTER">
                                    <font>
                                        <Font name="Smartisan Maquette Bold" size="46.0" />
                                    </font>
                                </Label>
                            </center>
                        </BorderPane>
                        <BorderPane layoutX="239.0" layoutY="138.0" prefHeight="106.0" prefWidth="147.0">
                            <center>
                                <Label fx:id="Complete_Step" text="Score" textFill="#96a2ff" BorderPane.alignment="CENTER">
                                    <font>
                                        <Font name="Smartisan Maquette Bold" size="68.0" />
                                    </font>
                                </Label>
                            </center>
                        </BorderPane>
                    </children>
                </Group>
            </children>
        </Group>
    </center>
</BorderPane>
