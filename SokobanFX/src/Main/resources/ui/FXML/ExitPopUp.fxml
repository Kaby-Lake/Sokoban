<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.Group?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane fx:id="Confirm_Exit_Pop_Up" prefHeight="720.0" prefWidth="1280.0" styleClass="Hide" stylesheets="@GlobalCSS.css" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ae2dms.UI.Game.PopUps.ExitPopUpController">
    <center>
        <Group BorderPane.alignment="CENTER">
            <children>
                <ImageView fitHeight="359.0" fitWidth="622.0">
                    <Image url="@../Assets/Game/ConfirmExitPopUp/Confirm_Exit.png" />
                </ImageView>
                <ImageView fx:id="Confirm_Exit_Back" layoutX="33.0" layoutY="280.0" styleClass="Button">
                    <Image url="@../Assets/Game/ConfirmExitPopUp/Back_To_Game.png" />
                </ImageView>
                <ImageView fx:id="Confirm_Exit_Exit" layoutX="405.0" layoutY="280.0" styleClass="Button">
                    <Image url="@../Assets/Game/ConfirmExitPopUp/Exit_Game.png" />
                </ImageView>
                <ImageView layoutX="218.0" layoutY="193.0" onMouseClicked="#clickSaveGame" styleClass="Button">
                    <Image url="@../Assets/Game/Save_Game.png" />
                </ImageView>
            </children></Group>
    </center>
</BorderPane>
